@startuml
struct AABB {
    + <<override>> ToString() : string
    + {static} Transform(transform:float4x4, localBounds:AABB) : AABB
}
class AABBExtensions <<static>> {
    + {static} ToAABB(bounds:Bounds) : AABB
    + {static} ToBounds(aabb:AABB) : Bounds
}
